{
  "name": "@afterdarksys/cli",
  "version": "1.0.0",
  "description": "Command-line interface for managing AfterDark Systems platform",
  "main": "dist/index.js",
  "bin": {
    "adsys": "./dist/cli/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "cli": "tsx src/cli/index.ts",
    "cli:build": "npm run build && chmod +x dist/cli/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "jsonwebtoken": "^9.0.2",
    "ora": "^7.0.1",
    "pg": "^8.11.3",
    "qrcode-terminal": "^0.12.0",
    "speakeasy": "^2.0.0",
    "table": "^6.8.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/commander": "^2.12.2",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.11.2",
    "@types/speakeasy": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.6.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afterdarksys/admin-cli.git"
  },
  "keywords": [
    "cli",
    "admin",
    "management",
    "afterdark",
    "nerdycupid",
    "undatable"
  ],
  "author": "AfterDark Systems <info@afterdarksys.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afterdarksys/admin-cli/issues"
  },
  "homepage": "https://github.com/afterdarksys/admin-cli#readme"
}
