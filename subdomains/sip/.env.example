# After Dark Systems SIP Gateway Configuration

# Server Configuration
PORT=3005
NODE_ENV=development

# AEIMS Integration
AEIMS_BASE_URL=http://localhost:8080
AEIMS_WEBHOOK_SECRET=your-webhook-secret-here

# After Dark Systems SSO
AFTER_DARK_AUTH_URL=https://login.afterdarksys.com/api
JWT_SECRET=your-jwt-secret-here

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://afterdarksys.com,https://sip.afterdarksys.com

# Development User Credentials (for local testing)
ADMIN_PASSWORD=admin123
OPERATOR_PASSWORD=operator123
USER_PASSWORD=user123

# Billing Configuration
CALL_RATE_PER_MINUTE=0.05

# Redis Configuration (optional)
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info
LOG_FILE=logs/sip-gateway.log

# FreeSWITCH Configuration (proxied through AEIMS)
FREESWITCH_HOST=localhost
FREESWITCH_PORT=8021
FREESWITCH_PASSWORD=ClueCon

# Security
WEBHOOK_SECRET=your-webhook-secret
SESSION_SECRET=your-session-secret
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=900000

# Monitoring
HEALTH_CHECK_INTERVAL=30000
METRICS_ENABLED=true